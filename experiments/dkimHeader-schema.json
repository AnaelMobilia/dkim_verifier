[
	{
		"namespace": "dkimHeader",
		"functions": [
			{
				"name": "setDkimHeaderResult",
				"type": "function",
				"description": "Show the given DKIM result in the header",
				"async": true,
				"parameters": [
					{
						"name": "tabId",
						"type": "number",
						"description": "Tab id"
					},
					{
						"name": "result",
						"type": "string",
						"description": "DKIM result"
					},
					{
						"name": "warnings",
						"type": "array",
						"items": {
							"type": "string"
						},
						"description": "DKIM warnings"
					},
					{
						"name": "faviconUrl",
						"type": "string",
						"description": "URL to the favicon to show"
					}
				]
			},
			{
				"name": "highlightFromAddress",
				"type": "function",
				"description": "Highlight the from address with the given text and background color",
				"async": true,
				"parameters": [
					{
						"name": "tabId",
						"type": "number",
						"description": "Tab id"
					},
					{
						"name": "color",
						"type": "string",
						"description": "text color"
					},
					{
						"name": "backgroundColor",
						"type": "string",
						"description": "background color"
					}
				]
			}
		]
	}
]
